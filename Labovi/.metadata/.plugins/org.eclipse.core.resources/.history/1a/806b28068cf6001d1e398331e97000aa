package rs.ac.bg.etf.kdp.sanja;

import java.util.concurrent.Semaphore;

public class Consumer extends Thread {
	
	private int N, cnt, buffer;
	private Semaphore mutex, empty, full;

	public Consumer(String name, int n, int cnt, int buffer, Semaphore mutex, Semaphore empty, Semaphore full) {
		super(name);
		N = n;
		this.cnt = cnt;
		this.buffer = buffer;
		this.mutex = mutex;
		this.empty = empty;
		this.full = full;
	}

}
